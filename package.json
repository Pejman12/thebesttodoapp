{
  "name": "todo-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome check",
    "format": "biome format --write",
    "db:generate": "env-cmd -e production -- drizzle-kit generate",
    "db:migrate": "env-cmd -e production -- drizzle-kit migrate",
    "db:push": "env-cmd -e production -- drizzle-kit push",
    "deploy:prod": "opennextjs-cloudflare deploy -- --env=\"\"",
    "deploy:dev": "opennextjs-cloudflare deploy -- --env dev",
    "upload": "opennextjs-cloudflare upload",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "build:dev": "env-cmd -e dev -- opennextjs-cloudflare build",
    "build:prod": "NODE_ENV=production env-cmd -e production -- opennextjs-cloudflare build",
    "preview": "opennextjs-cloudflare preview"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.31.9",
    "@neondatabase/serverless": "^1.0.1",
    "@opennextjs/cloudflare": "^1.8.0",
    "@tanstack/react-form": "^1.19.3",
    "@tanstack/react-query": "^5.87.1",
    "@tanstack/react-query-next-experimental": "^5.87.1",
    "@trpc/client": "^11.5.1",
    "@trpc/react-query": "^11.5.1",
    "@trpc/server": "^11.5.1",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "bufferutil": "^4.0.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.5",
    "lucide-react": "^0.542.0",
    "next": "^15",
    "react": "^19",
    "react-aria-components": "^1.12.1",
    "react-dom": "^19",
    "react-error-boundary": "^6.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "ws": "^8.18.3",
    "zod": "^4.1.5",
    "zod-form-data": "^3.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.3",
    "@tailwindcss/postcss": "^4.1.13",
    "@tanstack/react-query-devtools": "^5.87.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.1",
    "drizzle-kit": "^0.31.4",
    "env-cmd": "^11.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5",
    "wrangler": "^4.34.0"
  }
}
